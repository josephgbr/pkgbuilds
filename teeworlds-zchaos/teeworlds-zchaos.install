pkgname=teeworlds-zchaos

post_install() {
cat << EOMSG
    This package provides an example of server config.
    You can adapt it to your taste, copying to other
    more appropriate folder, or you can simply run:
     teeworlds-zchaos -f /usr/share/teeworlds/zchaos-srv.cfg
  
    It will be availble in the LAN tab of Teeworlds
  
EOMSG
}

post_upgrade() {
	post_install
}

