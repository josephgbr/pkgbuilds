pkgname=teeworlds-maple
pkgver=3
pkgrel=1
pkgdesc="Some stuff from Maple clan for TeeWorlds"
arch=(any)
url="http://themapleclan.6te.net/"
license=('GPL')
depends=('teeworlds')
source=("Maple.png" "Maple 2.png" "Maple_by_HMH.png" "BOX Maple.png" "DJ Maple.png" "Maple Cyborg.png" "Maple fire .png")
md5sums=('395d39204ae3370981a1874af8ea6c31'
         '8bcef7c0c353e1475e9b1de8062ec556'
         '7943ccc1d4822d4b9b6b636dc8d90ab7'
         '34e6c39ca684bd2c1ef7103f182b907c'
         '3ffbf5ddf4d357b4c1d1b2c5072a96e4'
         '60c365a8bf1da6d55b919b5341394263'
         'f2045167db133ab991220b542ce3c700')

package() {
  install -dm755 "$pkgdir"/usr/share/teeworlds/data/skins
  
  for i in "Maple.png" "Maple 2.png" "Maple_by_HMH.png" "BOX Maple.png" "DJ Maple.png" "Maple Cyborg.png" "Maple fire .png"; do
    install -m644 "$i" "$pkgdir"/usr/share/teeworlds/data/skins
  done
}
